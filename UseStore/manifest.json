{
    "manifest_version": 3,
    "name": "useStore",
    "version": "0.0.1",
    "description": "这是一个聚焦",
    "icons": {
        "16": "static/imgs/icon.png",
        "32": "static/imgs/icon.png",
        "48": "static/imgs/icon.png",
        "128": "static/imgs/icon.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": ["storage", "cookies", "tabs"],
    "host_permissions": [
        "*://*.xunfs.com/*",
        "http://192.168.110.49:5000/*",
        "*://*/*",
        "<all_urls>"
    ]
}
